<resources>
    <!-- Base application theme. -->
    <style name="GridTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <attr name="maxNumber" format="integer" />
    <attr name="lineNumber" format="integer" />
    <declare-styleable name="GridSelectPhotoView">
        <attr name="maxNumber" />
        <attr name="lineNumber" />
    </declare-styleable>
    <declare-styleable name="GridShowPhotoView">
        <attr name="lineNumber" />
    </declare-styleable>


</resources>